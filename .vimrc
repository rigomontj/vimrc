:set number
:set relativenumber
:set autoindent
:set ts=4 sw=4
:set cc=76
:set shell=/bin/bash

:syntax on
:highlight ColorColumn ctermbg=7

:inoremap jk <esc>
:inoremap <Up> <Nop>
:inoremap <Down> <Nop>
:inoremap <Left> <Nop>
:inoremap <Right> <Nop>

:noremap <Up> <Nop>
:noremap <Down> <Nop>
:noremap <Left> <Nop>
:noremap <Right> <Nop>
